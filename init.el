;;  *CONTEXT MENU*
;; [1] - Emacs client
;;     [1.1] - Misc
;;     [1.2] - Built in Options
;; [2] - Packages
;;     [2.1] - Setting up packages
;;     [2.2] - Misc
;;     [2.3] - Languages
;;     [2.4] - Visual/ Aesthetic

;; ================================================================== [1] - Emacs client ================================================================
;; ---------------                                   ---------------  [1.1] - "Misc"
;; Load theme
(load-theme 'wombat t)

;; Set default font face
(set-face-attribute 'default nil :font "Cascadia Mono")

;; ---------------                                   ---------------- [1.2] - "Built in Options"
;; Disable the menu bar
(menu-bar-mode -1)

;; Disable the tool bar
(tool-bar-mode -1)

;; Disable the scroll bars
(scroll-bar-mode -1)

;; Disables tooltips 💩
(tooltip-mode -1)

;; Disable splash screen
(setq inhibit-startup-screen t)

;; Enable mouse-support.
(setq eat-enable-mouse t)

;; Enable CUA key bindings (normal Copy/Cut/Past keybindings)
(cua-mode t)

;; Abbreviates yes/no to y/n
(defalias 'yes-or-no #'y-or-n-p)

;; Saves a Recent files list
(recentf-mode t)
(setq recentf-max-saved-items 10000)

;; Remembers previous user input (file selection etc.)
(savehist-mode t)

;; Remembers last position in files
(save-place-mode t)

;; Resize by frame, window and pixels, not by char
(setq frame-resize-pixelwise t)
(setq window-resize-pixelwise t)

;; Clean scratch buffer
(setq initial-scratch-message nil)

;; Peace at last 💩 (Disable Emacs noise on error)
(setq ring-bell-function 'ignore)

;; A common optimization is to temporarily disable garbage collection during initialization.
(setq gc-cons-threshold (* 100 1024 1024))

;; Optimisation (from Doom's core.el). See that file for descriptions.
(setq idle-update-delay 1.0)

;; Fullscreen on startup 
(add-to-list 'default-frame-alist '(fullscreen . maximized))

;; Removes minibuff tooltip
(setq inhibit-startup-echo-area-message "Solarized-Incog")

;; Autosaves files in a single directory "$HOME/.emacs.d/autosaves"
(defvar emacs-autosave-directory
  (concat user-emacs-directory "autosaves/")
  "This variable dictates where to put auto saves. It is set to a
  directory called autosaves located wherever your .emacs.d/ is
  located.")
(setq backup-directory-alist
      `((".*" . ,emacs-autosave-directory))
      auto-save-file-name-transforms
      `((".*" ,emacs-autosave-directory t)))

;; Guess major mode from file name
(setq-default major-mode
              (lambda () 
                (unless buffer-file-name
                  (let ((buffer-file-name (buffer-name)))
                    (set-auto-mode)))))

;; ================================================================== [2] - Packages ====================================================================
;; ---------------                                   ---------------- [2.1] - "Setting up packages"
;; Adding package source MELPA, (ELPA is added by default)
(require 'package)
(add-to-list 'package-archives '("melpa" . "https://melpa.org/packages/"))

;; Use package setup: Docs: https://github.com/jwiegley/use-package#installing-use-package
(eval-when-compile
  (require 'use-package))

;; ---------------                                   ---------------- [2.2] - "Misc"
;; ---------------                                   ---------------- [2.3] - "(Programing) Language support"
;; ---------------                                   ---------------- [2.4] - "Visual/ Aesthetic"
;; DASHBOARD: Docs https://github.com/emacs-dashboard/emacs-dashboard
;; Setup
(use-package dashboard
  :ensure t
  :config
  (dashboard-setup-startup-hook))
;; Config
(setq dashboard-banner-logo-title nil)
(setq dashboard-startup-banner "C:\\Users\\Solarized-Incog\\.emacs.d\\lambdacs-assets\\out.png")
(setq dashboard-center-content t)
(setq dashboard-set-init-info nil)
(setq dashboard-set-footer nil)
(setq dashboard-items '((recents  . 1)
                        (projects . 5)))

